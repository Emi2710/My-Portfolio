{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/FaqQuestions.tsx", "src/pages/_document.jsx", "src/pages/_app.jsx", "src/pages/api/hello.js", "services/sendMail.js", "components/Test.tsx", "src/pages/api/contact.js", "components/ContactForm.jsx"],
  "exclude": ["node_modules"]
}
